{
  "name": "@imwebme/eslint-config",
  "version": "1.1.15",
  "description": "imweb ESLint configs for Javascript",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "eslint . --config=eslintrc.json",
    "test": "npm run lint && tape test/*.js",
    "build": "nanobundle build",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imwebme/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "imwebme",
    "javascript"
  ],
  "author": "imwebme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imwebme/eslint-config/issues"
  },
  "homepage": "https://github.com/imwebme/eslint-config#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.2.1",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "eslint": "^7.23.0 || ^8.10.0",
    "typescript": "^4.6.2"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@actions/exec": "^1.1.1",
    "@changesets/cli": "^2.26.0",
    "nanobundle": "^1.5.0"
  },
  "exports": {
    ".": {
      "source": "./lib/index.js",
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    },
    "./recommended": {
      "source": "./lib/recommended.js",
      "require": "./lib/recommended.cjs",
      "import": "./lib/recommended.mjs"
    },
    "./typescript": {
      "source": "./lib/typescript.js",
      "require": "./lib/typescript.cjs",
      "import": "./lib/typescript.mjs"
    },
    "./react": {
      "source": "./lib/react.js",
      "require": "./lib/react.cjs",
      "import": "./lib/react.mjs"
    },
    "./prettier": {
      "source": "./lib/prettier.js",
      "require": "./lib/prettier.cjs",
      "import": "./lib/prettier.mjs"
    },
    "./zynk-react": {
      "source": "./lib/zynk-react.js",
      "require": "./lib/zynk-react.cjs",
      "import": "./lib/zynk-react.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "typescript",
    "react",
    "prettier"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
